<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
   <localRepository>./m2</localRepository>
   <interactiveMode>false</interactiveMode>
   <proxies>
      <proxy>
         <id>local</id>
         <active>true</active>
         <protocol>http</protocol>
         <host>internet.proxy.generic.com</host>
         <port>80</port>
         <nonProxyHosts>localhost|*.fedex.com</nonProxyHosts>
      </proxy>
   </proxies>
   <servers>
      <server>
         <id>releases</id>
         <username>dev</username>
         <password>password</password>
      </server>
      <server>
         <id>snapshots</id>
         <username>dev</username>
         <password>password</password>
      </server>
   </servers>
   <mirrors>
      <mirror>
         <id>nexus</id>
         <mirrorOf>central</mirrorOf>
         <url>https://genericnexus:8443/nexus/repository/maven-central</url>
      </mirror>
   </mirrors>
   <profiles>
      <profile>
         <id>nexus</id>
         <repositories>
            <repository>
               <id>snapshots</id>
               <url>https://genericnexus:8443/nexus/content/repositories/fxg-snapshots</url>
               <releases>
                  <enabled>false</enabled>
                  <updatePolicy>always</updatePolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <updatePolicy>always</updatePolicy>
               </snapshots>
            </repository>
            <repository>
               <id>corp-nexus-central</id>
               <url>https://nexus.prod.cloud.fedex.com:8443/nexus/content/repositories/central</url>
               <releases>
                  <updatePolicy>always</updatePolicy>
                  <enabled>false</enabled>
               </releases>
               <snapshots>
                  <updatePolicy>always</updatePolicy>
                  <enabled>false</enabled>
               </snapshots>
            </repository>
            <!--  necessary for JMS decorator  -->
            <repository>
               <id>jms</id>
               <name>JMS</name>
               <url>https://genericnexus:8443/nexus/content/repositories/3535157-FedexJmsDecorator</url>
               <releases>
                  <updatePolicy>always</updatePolicy>
               </releases>
               <snapshots>
                  <updatePolicy>always</updatePolicy>
               </snapshots>
            </repository>
            <!--  necessary for Oracle 12 upgrade ojdbc  -->
            <repository>
               <id>oracle</id>
               <name>oracle</name>
               <url>https://genericnexus:8443/nexus/content/repositories/oracle</url>
               <releases>
                  <updatePolicy>always</updatePolicy>
               </releases>
               <snapshots>
                  <updatePolicy>always</updatePolicy>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>snapshots</id>
               <url>https://genericnexus:8443/nexus/content/repositories/fxg-snapshots</url>
               <snapshots>
                  <updatePolicy>always</updatePolicy>
               </snapshots>
               <releases>
                  <updatePolicy>always</updatePolicy>
               </releases>
            </pluginRepository>
            <pluginRepository>
               <id>releases</id>
               <url>https://genericnexus:8443/nexus/content/repositories/fxg-releases</url>
               <snapshots>
                  <updatePolicy>always</updatePolicy>
               </snapshots>
               <releases>
                  <updatePolicy>always</updatePolicy>
               </releases>
            </pluginRepository>
         </pluginRepositories>
      </profile>
   </profiles>
   <activeProfiles>
      <activeProfile>nexus</activeProfile>
   </activeProfiles>
</settings>